<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>PremierWebv2</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>toto</servlet-name>
    <servlet-class>fr.afcepf.al31.controller.PremierServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>toto</servlet-name>
    <url-pattern>/b.chauvet.aspx</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FormulaireServlet</servlet-name>
    <servlet-class>fr.afcepf.al31.controller.FormulaireServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormulaireServlet</servlet-name>
    <url-pattern>/formulaire.html</url-pattern>
  </servlet-mapping>
  
</web-app>